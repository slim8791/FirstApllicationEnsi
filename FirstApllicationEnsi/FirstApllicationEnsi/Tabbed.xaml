<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="FirstApllicationEnsi.Tabbed"
            xmlns:local="clr-namespace:FirstApllicationEnsi;assembly=FirstApllicationEnsi">
    <TabbedPage.Children>
        <ContentPage Title="Page 1 ">
            <Label Text="Page 1"></Label>
        </ContentPage>
        <ContentPage Title="Page 2 ">
            <Label Text="Page 2"></Label>
        </ContentPage>
        <ContentPage Title="Page 3 ">
            <Label Text="Page 3"></Label>
        </ContentPage>

        <local:PhoneKeyboard></local:PhoneKeyboard>
        <local:MainPage></local:MainPage>
        <local:StackLayoutPage></local:StackLayoutPage>
    </TabbedPage.Children>
   
</TabbedPage>